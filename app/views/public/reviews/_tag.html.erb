      <table class="table table-bordered">
        <tr>
         <th>タグ検索</th>
        </tr>
        <% tags.each do |tag| %>
          <tr>
            <td><%= link_to tag_path(tag.id) do %>
                  <%= tag.name %>
                <% end %>
            </td>
          </tr>
        <% end %>
      </table>